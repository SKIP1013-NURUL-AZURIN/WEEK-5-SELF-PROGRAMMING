package selfProgramming;

public class Slide15 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double amount = 23.50;
		double balance = 50.10;
		
		if (amount <= balance) 
			balance = balance - amount;   
		
		System.out.println("Acct new balance = " + balance);
		
	}

}
